{
  "schemaVersion": 1,
  "id": "inventorytabs",
  "version": "${version}",

  "name": "Inventory Tabs",
  "description": "Adds tabs to your inventory that lead to nearby blocks.",
  "authors": [
    "KQP"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/inventory-tabs",
    "sources": "https://github.com/cakewhip/inventorytabs/",
    "issues": "https://github.com/cakewhip/inventorytabs/issues"
  },

  "license": "MIT",
  "icon": "assets/inventorytabs/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.kqp.inventorytabs.init.InventoryTabs"
    ],
    "client": [
      "com.kqp.inventorytabs.init.InventoryTabsClient"
    ]
  },
  "mixins": [
    "inventorytabs.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.5"
  },
  "suggests": {
    "flamingo": "*"
  },
  "custom": {
    "modupdater": {
      "strategy": "curseforge",
      "projectID": 400285
    }
  }
}
